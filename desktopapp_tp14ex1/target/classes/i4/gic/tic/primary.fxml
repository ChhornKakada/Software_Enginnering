<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="226.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="i4.gic.tic.PrimaryController">
   <children>
      <Label text="Login Form">
         <font>
            <Font size="18.0" />
         </font></Label>
      <TextField alignment="TOP_LEFT" prefHeight="40.0" prefWidth="280.0" promptText="Username">
         <font>
            <Font size="16.0" />
         </font>
         <opaqueInsets>
            <Insets bottom="50.0" left="25.0" right="10.0" top="5.0" />
         </opaqueInsets></TextField>
      <PasswordField prefHeight="40.0" prefWidth="280.0" promptText="password">
         <font>
            <Font size="16.0" />
         </font></PasswordField>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" prefHeight="25.0" prefWidth="91.0" text="Login">
         <font>
            <Font size="16.0" />
         </font></Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
